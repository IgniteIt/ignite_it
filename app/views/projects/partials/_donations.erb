<%= [1,2,3,4].sample %>

<% if project.was_paid?(current_user) %>
  You have paid
<% elsif project.is_payable_by(current_user) %>
  <%= link_to_modal 'Pay', new_project_charge_path(project), class: 'btn btn-success donate_btn' %>
<% elsif project.has_expired? %>
  Project expired!
<% else %>
  <%= link_to_modal 'Donate', new_project_donation_path(project), class: 'btn btn-success donate_btn' %>
<% end %>