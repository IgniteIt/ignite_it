<% if project.is_owner?(current_user) %>
  <%= link_to_modal "Edit", edit_project_path(project), :class=>"button" %>
  <%= link_to 'Delete', project_path(project), method: :delete %>
<% else %>
  <p>This project was created by <%= project.user.username %></p>
  <%= image_tag project.user.avatar.url(:thumb), :id => "creator_avatar"%>
<% end %>
