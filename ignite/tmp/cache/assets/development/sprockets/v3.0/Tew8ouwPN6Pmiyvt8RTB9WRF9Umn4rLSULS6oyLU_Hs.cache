I"<(function() {
  var Timers;

  Timers = {
    init: function() {
      this.initCountdownUntil();
      return this.initCountdownSince();
    },
    initCountdownUntil: function() {
      return $('div[data-countdown-until]').each(function(index, element) {
        var $element, date;
        $element = $(element);
        date = new Date($element.data('countdown-until'));
        return $element.countdown({
          until: date
        });
      });
    },
    initCountdownSince: function() {
      return $('div[data-countdown-since]').each(function(index, element) {
        var $element, date;
        $element = $(element);
        date = new Date($element.data('countdown-since'));
        return $element.countdown({
          since: date
        });
      });
    }
  };

  window.Timers = Timers;

}).call(this);
